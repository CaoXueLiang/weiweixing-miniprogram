
.coverImage {
  position: absolute;
  top: calc(50% - 36px);
  left: calc(50% - 20px);
  width: 40px;
  height: 48px;
}

.location_image{
  width: 94rpx;
  height: 94rpx;
  position: absolute;
  bottom: 40rpx;
  left: 40rpx;
}

.scan_image{
  width: 80px;
  height: 80px;
  position: absolute;
  bottom: 40rpx;
  left: calc(50% - 40px);
}

.bottom_contain_view{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
  padding: 30rpx;
}

.font_18_black{
  font-size: 38rpx;
  color: black;
}

.font_13_666666{
  font-size: 26rpx;
  color: #555555;
}

.font_13_999999{
  font-size: 26rpx;
  color: #999999;
}

.flex_row_wrap{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: center;
  margin-top: 10rpx;
}

.tag_style{
  height: 34rpx;
  line-height: 34rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  text-align: center;
  border-radius: 17rpx;
  border-style: solid;
  border-width: 2rpx;
  border-color: #999999;
  color: #666666;
  font-size: 24rpx;
}

.margin_left_10{
  margin-left: 10rpx;
}